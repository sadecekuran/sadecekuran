<script>
    window.onscroll = function() { scrollSticky() };

    var stickyHeader = document.getElementById('header');
    var stickyHeaderBottom = document.getElementById('header-bottom');
    var stickyNotification = document.getElementById('notification');
    var stickyToast = document.getElementById('swal-toast');
    var stickyEl = stickyHeaderBottom.offsetTop;

    function scrollSticky () {
        if (window.pageYOffset >= stickyEl) {
            stickyHeader.classList.add('header-sticky');
            stickyNotification.classList.add('notification-sticky');
            stickyToast.classList.add('toast-sticky');
        } else {
            stickyHeader.classList.remove('header-sticky');
            stickyNotification.classList.remove('notification-sticky');
            stickyToast.classList.remove('toast-sticky');
        }
    }
</script>


