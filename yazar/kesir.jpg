<script>
        document.addEventListener('DOMContentLoaded', function() {
        if (preG && preG.LS_IS_AVAILABLE) {
            const recentlyViewedProductsList = localStorage.getItem('recent_views');
            let startTranslate = null;
            let endTranslate = null;
            
            if (recentlyViewedProductsList) {
                const recentlyViewedCr = document.querySelector(
                    '.recently-viewed-cr'
                );
                
                const clearRecentlyViewedButton = recentlyViewedCr
                    .querySelector('.recently-clear')
                    .querySelector('a');
                
                const swiperCls = 'recently-products-swiper';
                const swiperEl = document.querySelector(`.${swiperCls}`);
                const swiperWrapper = swiperEl.querySelector('.swiper-wrapper');
                
                const clearRecentViewed = () => {
                    localStorage.removeItem('recent_views');
                    recentlyViewedCr.style.display = 'none';
                };
                
                clearRecentlyViewedButton.addEventListener('click', clearRecentViewed);
                
                const recentlyViewedElements = JSON.parse(
                    recentlyViewedProductsList
                )
                    .map((product) => {
                        const slideElement = document.createElement('div');
                        const element = document.createElement('a');
                        const productImage = document.createElement('img');
                        
                        productImage.setAttribute('src', product.image);
                        productImage.setAttribute('alt', product.name);
                        
                        element.setAttribute('class', 'recently-product');
                        element.setAttribute('href', product.href);
                        element.setAttribute('title', product.name);
                        element.insertAdjacentElement('afterbegin', productImage);
                        
                        slideElement.setAttribute('class', 'swiper-slide');
                        slideElement.insertAdjacentElement('afterbegin', element);
                        
                        // eslint-disable-next-line eqeqeq
                        if (typeof preU !== 'undefined' && (!preU.prId || preU.prId != product.product_id)) return slideElement;
                        
                        if (Date.now() - product.available_at > 5000) return slideElement;
                        
                        return undefined;
                    })
                    .filter((product) => product);
                
                if (recentlyViewedElements.length > 0) {
                    recentlyViewedCr.style.display = 'block';
                    
                    recentlyViewedElements.forEach((element) => {
                        swiperWrapper.insertAdjacentElement(
                            'beforeend',
                            element
                        );
                    });
                    
                    const nextButton = document.querySelector('.recently-viewed-cr .next');
                    const prevButton = document.querySelector('.recently-viewed-cr .prev');
                    
                    const swiper = new Swiper(swiperEl, {
                        direction: 'horizontal',
                        slidesPerView: 'auto',
                        freeMode: true,
                        scrollbar: {
                            el: document.querySelector(`.${swiperCls} .swiper-scrollbar`),
                            hide: true
                        },
                        slidesOffsetBefore: 2,
                        slidesOffsetAfter: 20,
                        navigation: {
                            nextEl: nextButton,
                            prevEl: prevButton
                        }
                    });
                    
                    const updatePreventClicks = () => {
                        const distance = Math.abs(endTranslate - startTranslate);
                        
                        if (distance <= 10) {
                            swiper.params.preventClicks = false;
                            swiper.params.preventClicksPropagation = false;
                            swiper.update();
                        } else {
                            swiper.params.preventClicks = true;
                            swiper.params.preventClicksPropagation = true;
                        }
                        
                        startTranslate = null;
                        endTranslate = null;
                    };
                    
                    swiper.on('sliderFirstMove', () => {
                        startTranslate = Math.abs(swiper.getTranslate());
                    });
                    
                    swiper.on('touchEnd', () => {
                        endTranslate = Math.abs(swiper.getTranslate());
                        updatePreventClicks();
                    });
                }
            }
        }
    });
</script>
        
