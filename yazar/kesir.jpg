<script>
    function getURLVar(urlVarName) {
        var urlHalves = String(document.location)
            .toLowerCase()
            .split("?");
        var urlVarValue = "";
        if (urlHalves[1]) {
            var urlVars = urlHalves[1].split("&");
            for (var i = 0; i <= urlVars.length; i++) {
                if (urlVars[i]) {
                    var urlVarPair = urlVars[i].split("=");
                    if (
                        urlVarPair[0] &&
                        urlVarPair[0] == urlVarName.toLowerCase()
                    ) {
                        urlVarValue = urlVarPair[1];
                    }
                }
            }
        }

        return urlVarValue;
    }

    function removeProduct(key, route) {
        const cartElement = document.querySelector("#cart");
        if (route === 'checkout/cart' || route === 'checkout/checkout') {
            location.href = 'index.php?route=checkout/cart&remove=' + key;
        } else {
            fetch('index.php?route=module/cart&remove=' + key + '&clicked=true')
            .then(response => response.text())
            .then(data => {
                const tempDiv = document.createElement('div');
                tempDiv.innerHTML = data;

                const heading = tempDiv.querySelector("#cart .heading");
                const content = tempDiv.querySelector("#cart .content");

                cartElement.innerHTML = '';
                if (heading) cartElement.appendChild(heading);
                if (content) cartElement.appendChild(content);

                cartElement.classList.add("active");
                document.querySelector(".content #waiting").style.display = 'none';
                document.querySelector("#cart-content").style.display = 'block';
            });
        }
    }
</script>
