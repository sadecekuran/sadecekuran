        <script>
                        document.addEventListener('DOMContentLoaded', function () {
                const bankInformationButtons = document.querySelectorAll('.js-bank-transfer');
                if (bankInformationButtons !== null) {
                    bankInformationButtons.forEach((button) => {
                        button.addEventListener('click', () => {
                            $.ajax({
                                url: 'index.php?route=information/information/info&information_id=72',
                            }).then((res) => {
                                    Swal.fire({
                                        html: `<div style="display: flex; justify-content: center;">${res}</div>`,
                                        showConfirmButton: false,
                                        showCloseButton: true,
                                        closeButtonHtml: '&times;',
                                        customClass: {
                                            popup: 'swal-popup-custom',
                                            htmlContainer: 'swal-html-custom',
                                            closeButton: 'my-custom-close-button',
                                        },
                                    });
                                })
                                .catch((err) => {
                                    Sentry.captureException(
                                        new Error(
                                            `Exception on checkout success bank information modal - message: ${err.message}`
                                        )
                                    );
                                });
                        });
                    });
                }
            });
        </script>
        
